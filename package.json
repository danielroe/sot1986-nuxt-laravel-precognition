{
  "name": "@sot/nuxt-laravel-precognition",
  "type": "module",
  "version": "0.0.2",
  "description": "Nuxt module to work with Laravel Precognition and $fetch",
  "author": {
    "name": "Sot",
    "email": "sot@8dplus.com"
  },
  "license": "MIT",
  "repository": "https://github.com/sot1986/nuxt-laravel-precognition.git",
  "keywords": [
    "Nuxt",
    "Laravel",
    "Precognition",
    "Nuxt-Precognition",
    "ofetch"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release:locally": "npm run lint && npm run test && npm run prepack && npm pack && mv ./sot-nuxt-laravel-*.tgz ../Released",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@vueuse/core": "^10.3.0",
    "defu": "^6.1.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isequal": "^4.0.7",
    "@types/lodash.merge": "^4.0.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "^18.17.3",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "changelogen": "^0.5.4",
    "eslint": "^8.47.0",
    "happy-dom": "^10.11.0",
    "nuxt": "^3.6.5",
    "playwright-core": "^1.37.1",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.8",
    "zod": "^3.22.2"
  }
}